<?xml version="1.0" encoding="GBK"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName">

    <bean id="defaultSequence" class="org.chmcc.common.util.Sequence"/>

    <bean id="sequenceUtil" class="org.chmcc.common.util.SequenceUtil">
        <property name="defaultSequence" ref="defaultSequence"/>
    </bean>


    <bean id="sqlMapClient" class="org.springframework.orm.ibatis.SqlMapClientFactoryBean">
        <property name="configLocation" value="classpath:sqlmap-config.xml"/>
    </bean>

    <bean id="exampleDao" class="org.chmcc.helloworld.dao.example.impl.ExampleDaoImpl"/>


</beans>