{
   "background": {
      "scripts": [ "header.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon32.png",
      "default_title": "Local Explorer"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "header.js", "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com https://www.google-analytics.com https://ssl.google-analytics.com; object-src https://www.google.com https://www.google-analytics.com https://ssl.google-analytics.com 'self'",
   "default_locale": "en",
   "description": "__MSG_extDescription__",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Local Explorer - File Manager on web browser",
   "options_page": "options.html",
   "permissions": [ "tabs", "storage", "http://*/*", "https://*/*" ],
   "short_name": "Local Explorer",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2016.6.21.0"
}
