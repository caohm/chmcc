{
  "manifest_version": 2,
  "name": "实时单量数据监控",
  "version": "0.0.1",
  "background": { "scripts": ["jquery-2.0.0.min.js","util.js", "background.js"] },
  "icons": {
	"16": "chart_line.png",
	"48": "chart_line.png",
	"128": "chart_line.png"
  },
  "page_action": {
	  "default_icon": {
		"16": "chart_line.png",
		"48": "chart_line.png",
		"128": "chart_line.png"
	  },
	  "default_title": "实时单量数据监控", // shown in tooltip
	  "default_popup": "popup.html"
  },
  "permissions" : ["tabs", "http://*.jd.com/*", "http://localhost/*"],
  "content_scripts":[{
	  "matches":["http://risking.jd.com/*"],
	  "js":["jquery-2.0.0.min.js", "content_script.js", "util.js"]
  }]
}