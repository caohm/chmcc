{
  "manifest_version": 2,
  "name": "上线助手",
  "version": "0.0.1",
  "background": { "scripts": ["jquery-2.0.0.min.js","util.js", "background.js"] },
  "icons": {
	"16": "gear.png",
	"48": "gear.png",
	"128": "gear.png"
  },
  "page_action": {
	  "default_icon": {
		"16": "gear.png",
		"48": "gear.png",
		"128": "gear.png"
	  },
	  "default_title": "上线助手", // shown in tooltip
	  "default_popup": "popup.html"
  },
  "permissions" : ["tabs", "http://*.jd.com/*", "http://localhost/*"],
  "content_scripts":[{
	  "matches":["http://jsf.jd.com/*", "http://deploy.jd.com/*"],
	  "js":["jquery-2.0.0.min.js", "content_script.js", "util.js"]
  }]
}