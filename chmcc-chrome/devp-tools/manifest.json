{
    "name": "devp_help",
    "short_name": "devp_help",
    "version": "1.0",
    "manifest_version": 2,
    "description": "this is devp helper.",
    "icons": {
        "16": "assets/icon_16.png",
        "48": "assets/icon_128.png",
        "128": "assets/icon_128.png"
    },
    "background": {
        "scripts": []
    },
    "homepage_url": "https://github.com/helibao",
    "page_action": {
        "default_popup": ""
    },
    "permissions": [
        "tabs",
        "*://*/*",
        "contextMenus",
        "declarativeContent",
        "storage"
    ],
    "update_url": "https://github.com/helibao/devp-tools.git",
    "content_scripts": [
        {
            "matches": [
                "*://ump.jd.com/performanceWithMachine/*",
                "*://deploy.jd.com/welcome/*",
                "*://logbook.jd.com/live/*"
            ],
            "js": [
                "js/jquery-1.10.2.min.js",
                "js/ump-helper.js",
                "js/deploy-log-helper.js",
                "js/log-helper.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "assets/UNIX_TS_2.png"
    ]
}
